    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    callsub get_templated_randomness_beacon_id
    itob
    byte 0x151f7c75
    swap
    concat
    log
    int 1
    return

main_get_templated_safety_round_gap_route@3:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    callsub get_templated_safety_round_gap
    itob
    byte 0x151f7c75
    swap
    concat
    log
    int 1
    return

main_commit_route@4:
