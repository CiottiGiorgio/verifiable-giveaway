#pragma version 10

smart_contracts.verifiable_shuffle.contract.VerifiableShuffle.approval_program:
    txn NumAppArgs
    bz main_bare_routing@8
    method "get_templated_randomness_beacon_id()uint64"
    method "get_templated_safety_round_gap()uint64"
    method "commit(uint8,uint32,uint8)void"
    method "reveal()(byte[32],uint32[])"
    txna ApplicationArgs 0
    match main_get_templated_randomness_beacon_id_route@2 main_get_templated_safety_round_gap_route@3 main_commit_route@4 main_reveal_route@5
    err // reject transaction

main_get_templated_randomness_beacon_id_route@2:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    callsub get_templated_randomness_beacon_id
    itob
    byte 0x151f7c75
    swap
    concat
    log
    int 1
    return

main_get_templated_safety_round_gap_route@3:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    callsub get_templated_safety_round_gap
    itob
    byte 0x151f7c75
    swap
    concat
    log
    int 1
    return

main_commit_route@4:
    int 1
    txn OnCompletion
    shl
    int 3
    &
    assert // OnCompletion is one of NoOp, OptIn
    txn ApplicationID
    assert // is not creating
    txna ApplicationArgs 1
    txna ApplicationArgs 2
    txna ApplicationArgs 3
    callsub commit
    int 1
    return

main_reveal_route@5:
    int 1
    txn OnCompletion
    shl
    int 5
    &
    assert // OnCompletion is one of NoOp, CloseOut
    txn ApplicationID
    assert // is not creating
    callsub reveal
    byte 0x151f7c75
    swap
    concat
    log
    int 1
    return

main_bare_routing@8:
    txn OnCompletion
    switch main_create@9 main_reject_bare_on_completion@12 main_reject_bare_on_completion@12 main_reject_bare_on_completion@12 main_update@10 main_delete@11
    err // reject transaction

main_create@9:
    txn ApplicationID
    !
    assert // is creating
    int 1
    return

main_update@10:
    txn ApplicationID
    assert // is not creating
    callsub update
    int 1
    return

main_delete@11:
    txn ApplicationID
    assert // is not creating
    callsub delete
    int 1
    return

main_reject_bare_on_completion@12:
    err // reject transaction


